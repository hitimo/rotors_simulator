<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="zuerifalse">

    <gui>
      <camera name="user_camera">
        <pose>500 -350 2.000000 0.000000 0.243643 3.141593</pose>
      </camera>
      <plugin name="view_selection" filename="librotors_gazebo_view_control_plugin.so"/>
      <!--<plugin name="map_tracker" filename="librotors_gazebo_map_tracker_plugin.so"/>-->
    </gui>

    <scene>
      <ambient>0.2 0.2 0.2 1.0</ambient>
      <grid>false</grid>
      <sky>
        <clouds>
          <speed>4</speed>
        </clouds>
      </sky>
    </scene>

    <physics type='ode'>
      <ode>
        <solver>
          <type>quick</type>
          <iters>250</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100</contact_max_correcting_vel>
          <contact_surface_layer>0.01</contact_surface_layer>
        </constraints>
      </ode>
      <max_step_size>0.01</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
      <gravity>0 0 -9.81</gravity>
    </physics>


  <!-- Light Source -->
    <light type="directional" name="sun">
      <cast_shadows>false</cast_shadows>
      <pose>0 0 300 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.0 0.0 0.0 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>0.2 0.2 -0.9</direction>
    </light>



  <!-- This is the actual world -->
    <include>
      <uri>model://zuerifalse</uri>
    </include>
    <!-- Fetches the sdf model present in models/zuerifalse -->


  <!-- This is a grass plane for takeoff. Note: collision plane spans over whole map...-->
    <model name="grass_takeoff_plane">
      <static>true</static>
      <link name='link'>
      <collision name='collision'>
        <pose>460 -350 0 0 0 0</pose>
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>80 20</size>
          </plane>
        </geometry>
        <surface>
            <friction>
              <ode>
                <mu>0.2</mu>
                <mu2>.2</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name='visual_1'>
          <pose>460 -350 0 0 0 0</pose>
          <cast_shadows>false</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>80 20</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>model://grass_plane/materials/scripts</uri>
              <uri>model://grass_plane/materials/textures</uri>
              <name>vrc/grass</name>
            </script>
          </material>
        </visual>
      </link>
    </model>
  </world>
</sdf>
